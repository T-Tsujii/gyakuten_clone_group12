<div class="row d-flex justify-content-center">
  <div class="mb-5">
    <% @movies.each do |movie| %>
      <div class="text-center">
        <p><%= movie[:title] %></p>
        <div>
          <iframe width="560" height="315" <%= video_tag "#{movie[:url]}"%> frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
      </div>
    <% end %>
  </div>

  <%= paginate @movies %>
</div>